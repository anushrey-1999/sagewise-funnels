{
  "id": "fa",
  "title": "Life Insurance Quote Calculator",
  "subtitle": "Complete the form below to receive personalized quotes.",
  "steps": [
    {
      "id": "zip-code",
      "title": "What is your zip code?",
      "description": "lorem ipsum dolor sit amet",
      "fields": [
        {
          "id": "zipCode",
          "type": "number",
          "label": "",
          "placeholder": "12345",
          "required": true,
          "autoForward": true,
          "validation": {
            "pattern": "^\\d{5}$",
            "message": "Zip code must be exactly 5 digits"
          }
        }
      ]
    },
    {
      "id": "gender",
      "title": "What is your gender?",
      "description": "lorem ipsum dolor sit amet",
      "fields": [
        {
          "id": "gender",
          "type": "radio",
          "label": "What is your Gender",
          "required": true,
          "options": [
            { "value": "male", "label": "Male" },
            { "value": "female", "label": "Female" }
          ]
        }
      ]
    },
    {
      "id": "age",
      "title": "What is your age?",
      "description": "Select your age range",
      "fields": [
        {
          "id": "age",
          "type": "select",
          "label": "",
          "required": true,
          "options": [
            { "value": "under-45", "label": "Under 45" },
            { "value": "45-49", "label": "45-49" },
            { "value": "50-54", "label": "50-54" },
            { "value": "55-59", "label": "55-59" },
            { "value": "60-64", "label": "60-64" },
            { "value": "65-69", "label": "65-69" },
            { "value": "70-74", "label": "70-74" },
            { "value": "75-79", "label": "75-79" },
            { "value": "80-85", "label": "80-85" },
            { "value": "over-85", "label": "Over 85" }
          ]
        }
      ]
    },
    {
      "id": "coverage",
      "title": "How much coverage do you need?",
      "description": "How much coverage do you need?",
      "fields": [
        {
          "id": "coverage",
          "type": "select",
          "label": "",
          "required": true,
          "options": [
            { "value": "0-4999", "label": "$0 - $4,999" },
            { "value": "5000-9999", "label": "$5000 - $9,999" },
            { "value": "10000-14999", "label": "$10,000 - $14,999" },
            { "value": "15000-19999", "label": "$15,000 - $19,999" },
            { "value": "20000-24999", "label": "$20,000 - $24,999" },
            { "value": "25000-plus", "label": "$25,000+" }
          ]
        }
      ]
    },
    {
      "id": "contact-info",
      "title": "You're Pre-Qualified For Major Discounts.",
      "description": "Complete this final step to find out how much you can save today!",
      "fields": [
        {
          "id": "firstName",
          "type": "text",
          "label": "First name",
          "placeholder": "John",
          "required": true
        },
        {
          "id": "lastName",
          "type": "text",
          "label": "Last name",
          "placeholder": "Doe",
          "required": true
        },
        {
          "id": "email",
          "type": "email",
          "label": "Email",
          "placeholder": "john.doe@email.com",
          "required": true
        }
      ]
    }
  ],
  "finalStep": {
    "title": "Thank you!",
    "description": "We're processing your information. You'll receive quotes shortly.",
    "buttonText": "Submit"
  }
}

